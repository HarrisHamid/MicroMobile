<h1>Create Listing</h1>

<form
  action="/vehicleListings/createListing"
  method="POST"
  id="createListingForm"
  enctype="multipart/form-data"
>
  <ul id="error-model" class="text-danger"></ul>

  <div class="form-columns">
    <!-- Left Column -->
    <div class="form-column">
      <label for="postTitle">Post Title</label>
      <input
        type="text"
        name="postTitle"
        id="postTitle"
        value="{{data.postTitle}}"
      />

      <label for="vehicleType">Vehicle Type</label>
      <select name="vehicleType" id="vehicleType">
        <option value="Scooter">Scooter</option>
        <option value="Skateboard">Skateboard</option>
        <option value="Bicycle">Bicycle</option>
        <option value="Other">Other</option>
      </select>

      <label for="vehicleTags1">First vehicle tag</label>
      <select name="vehicleTags1" id="vehicleTags1">
        <option value="None">None</option>
        <option value="Off Road">Off Road</option>
        <option value="Electric">Electric</option>
        <option value="Two Wheels">Two Wheels</option>
        <option value="Four Wheels">Four Wheels</option>
        <option value="New">New</option>
        <option value="Modded">Modded</option>
        <option value="Snow Gear">Snow Gear</option>
        <option value="Beach Gear">Beach Gear</option>
      </select>

      <label for="vehicleTags2">Second vehicle tag</label>
      <select name="vehicleTags2" id="vehicleTags2">
        <option value="None">None</option>
        <option value="Off Road">Off Road</option>
        <option value="Electric">Electric</option>
        <option value="Two Wheels">Two Wheels</option>
        <option value="Four Wheels">Four Wheels</option>
        <option value="New">New</option>
        <option value="Modded">Modded</option>
        <option value="Snow Gear">Snow Gear</option>
        <option value="Beach Gear">Beach Gear</option>
      </select>

      <label for="vehicleTags3">Third vehicle tag</label>
      <select name="vehicleTags3" id="vehicleTags3">
        <option value="None">None</option>
        <option value="Off Road">Off Road</option>
        <option value="Electric">Electric</option>
        <option value="Two Wheels">Two Wheels</option>
        <option value="Four Wheels">Four Wheels</option>
        <option value="New">New</option>
        <option value="Modded">Modded</option>
        <option value="Snow Gear">Snow Gear</option>
        <option value="Beach Gear">Beach Gear</option>
      </select>

      <label for="protectionIncluded">Is protection included in the rental</label>
      <select name="protectionIncluded" id="protectionIncluded">
        <option value="no">No</option>
        <option value="yes">Yes</option>

      </select>
    </div>

    <!-- Right Column -->
    <div class="form-column">
      <label for="vehicleCondition">Vehicle Condition (1.0-5.0)</label>
      <input
        type="number"
        step="0.1"
        name="vehicleCondition"
        id="vehicleCondition"
        value="{{data.vehicleCondition}}"
      />

      <label for="maxRentalHours">Max Rental Hours</label>
      <br />
      <input
        type="number"
        name="maxRentalHours"
        id="maxRentalHours"
        value="{{data.maxRentalHours}}"
      />
      <br />
      <label for="maxRentalDays">Max Rental Days</label>
      <br />
      <input
        type="number"
        name="maxRentalDays"
        id="maxRentalDays"
        value="{{data.maxRentalDays}}"
      />
      <br />
      <label for="hourlyCost">Cost Per Hour</label>
      <br />
      <input
        type="number"
        step="0.01"
        name="hourlyCost"
        id="hourlyCost"
        value="{{data.hourlyCost}}"
      />
      <br />
      <label for="dailyCost">Cost Per Day</label>
      <br />
      <input
        type="number"
        step="0.01"
        name="dailyCost"
        id="dailyCost"
        value="{{data.dailyCost}}"
      />
      <br />
      <label for="image">Image</label>
      <br />
      <input
        type="file"
        id="image"
        name="image"
        accept="image/png, image/jpeg"
        value="{{data.image}}"
      />

